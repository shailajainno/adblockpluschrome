{
  "author": "Gener8 Team",
  "manifest_version": 2,
  "name": "Gener8",
  "version": "1.0.4",
  "description": "Gener8 gives you control of your advertising experience.\nBlocking unwanted ads, showing you ads youâ€™re interested in and rewarding you when you see them.",
  "icons": {
    "128": "gener8/img/icon128.png",
    "16": "gener8/img/icon16.png",
    "48": "gener8/img/icon48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "gener8/img/icon19.png",
      "38": "gener8/img/icon38.png"
    },
    "default_popup": "gener8/popup/popup.html",
    "default_title": "Gener8"
  },
  "background": {
    "scripts": [
      "config.js",
      "gener8/bg_constants.js",
      "jquery-ui/js/jquery-3.2.1.min.js",
      "polyfill.js",
      "ext/common.js",
      "gener8/scheduler.js",
      "ext/background.js",
      "lib/compat.js",
      "lib/publicSuffixList.js",
      "lib/adblockplus.js",
      "devenvPoller__.js",
      "gener8/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "config.js",
        "jquery-ui/js/jquery-3.2.1.min.js",
        "gener8/js/globalVariable.js",
        "gener8/js/globalFunction.js",
        "gener8/js/addGener8Class.js",
        "polyfill.js",
        "ext/common.js",
        "ext/content.js",
        "include.preload.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "jquery-ui/js/jquery-3.2.1.min.js",
        "include.postload.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "gener8/js/auth.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "notifications",
    "cookies"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+8"
      }
    }
  }
}
